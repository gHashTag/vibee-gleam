{
  "name": "vibee-remotion",
  "version": "1.0.0",
  "description": "Remotion video compositions for VIBEE",
  "type": "module",
  "scripts": {
    "start": "remotion studio",
    "build": "remotion bundle",
    "render": "remotion render",
    "upgrade": "remotion upgrade",
    "captions": "tsx scripts/auto-captions.ts",
    "captions:force": "tsx scripts/auto-captions.ts --force",
    "predeploy": "npm run captions",
    "deploy": "npm run predeploy && fly deploy -a vibee-remotion"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
