    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@remotion/bundler": "4.0.388",
    "@remotion/captions": "^4.0.391",
    "@remotion/cli": "4.0.388",
    "@remotion/google-fonts": "^4.0.394",
    "@remotion/install-whisper-cpp": "^4.0.391",
    "@remotion/lambda": "4.0.388",
    "@remotion/renderer": "4.0.388",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/ws": "^8.18.1",
    "@vladmandic/face-api": "^1.7.15",
    "canvas": "^3.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remotion": "4.0.388",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.12",
